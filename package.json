{
  "name": "@am92/express-utils",
  "version": "1.0.10",
  "description": "Utilities for Express Framework",
  "type": "module",
  "main": "dist/ExpressUtils.js",
  "module": "src/ExpressUtils.mjs",
  "scripts": {
    "test": "jest",
    "build": "rimraf dist/ && babel src/ --out-dir dist/ --copy-files",
    "preversion": "pnpm test",
    "version": "pnpm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "repository": "git@github.com:heliumtank92/am92-express-utils.git",
  "homepage": "https://github.com/heliumtank92/am92-express-utils#readme",
  "contributors": [
    {
      "name": "Ankit Gandhi",
      "email": "ankitgandhi452@gmail.com",
      "url": "https://github.com/ankitgandhi452"
    },
    {
      "name": "Mehul Agarwal",
      "email": "agarwalmehul92@gmail.com",
      "url": "https://github.com/agarwalmehul"
    }
  ],
  "license": "MIT",
  "exports": {
    "./asyncWrapper": {
      "require": "./dist/lib/asyncWrapper.js",
      "default": "./src/lib/asyncWrapper.mjs"
    },
    "./configureRouter": {
      "require": "./dist/lib/configureRouter.js",
      "default": "./src/lib/configureRouter.mjs"
    },
    "./httpContext": {
      "require": "./dist/lib/httpContext.js",
      "default": "./src/lib/httpContext.mjs"
    },
    "./initializeRoutes": {
      "require": "./dist/lib/initializeRoutes.js",
      "default": "./src/lib/initializeRoutes.mjs"
    },
    "./CustomError": {
      "require": "./dist/classes/CustomError.js",
      "default": "./src/classes/CustomError.mjs"
    },
    "./ResponseBody": {
      "require": "./dist/classes/ResponseBody.js",
      "default": "./src/classes/ResponseBody.mjs"
    },
    "./CONSTANTS": {
      "require": "./dist/CONSTANTS.js",
      "default": "./src/CONSTANTS.mjs"
    }
  },
  "dependencies": {
    "@am92/api-crypto": "^1.0.7",
    "@am92/api-logger": "^1.0.5",
    "express-http-context": "^1.2.4",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.3.1",
    "babel-plugin-module-extension": "^0.1.3",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "express",
    "expressjs",
    "node",
    "api crypto",
    "api logging"
  ]
}
